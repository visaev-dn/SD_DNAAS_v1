# Example devices.yaml file
# Copy this file to devices.yaml and modify with your device information

defaults:
  username: your_username
  password: your_password
  ssh_port: 22

# Example Leaf Devices
DNAAS-LEAF-A01:
  mgmt_ip: 192.168.1.10
  location: A01
  role: leaf
  status: active

DNAAS-LEAF-A02:
  mgmt_ip: 192.168.1.11
  location: A02
  role: leaf
  status: active

# Example Spine Devices
DNAAS-SPINE-A01:
  mgmt_ip: 192.168.1.20
  location: A01
  role: spine
  status: active

DNAAS-SPINE-A02:
  mgmt_ip: 192.168.1.21
  location: A02
  role: spine
  status: active

# Example Super Spine Devices
DNAAS-SuperSpine-D04-NCC0:
  mgmt_ip: 192.168.1.30
  location: D04
  role: superspine
  status: active

DNAAS-SuperSpine-D04-NCC1:
  mgmt_ip: 192.168.1.31
  location: D04
  role: superspine
  status: active 